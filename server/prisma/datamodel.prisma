type User {
    id: ID! @unique @id
    name: String!
    email: String! @unique
    password: String!
    teams: [Team!]!
}

type Team {
    id: ID! @unique @id
    name: String!
    owner: User!
    players: [ID!]! @scalarList(strategy: RELATION)
    espnId: ID
}

type FantasyLeague {
    id: ID! @unique @id
    name: String!
    teams: [Team!]!
    espnId: ID! @unique
}
