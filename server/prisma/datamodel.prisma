type User {
    id: ID! @unique
    name: String!
    email: String! @unique
    password: String!
    teams: [Team!]!
}

type Team {
    id: ID! @unique
    name: String!
    owner: User!
    players: [ID!]!
}

type FantasyLeague {
    id: ID! @unique
    name: String!
    admin: User!
    teams: [Team!]!
}
